---
import Layout from '@/layouts/Layout.astro';
import BarBottom from "@/components/BarBottom.astro";
import Profile from "@/components/Profile.svelte";
---

<Layout title="export" >
    <main>
        <div>
            <Profile client:load />
        </div>
        <BarBottom exportPage />
    </main>
</Layout>

<style>
    main{
        display: flex;
        padding: 10px;
        height: 100vh;
        align-items: center;
        justify-content: space-between;
        flex-direction: column;
        overflow: hidden;
    }
    div{
        width: 100%;
        display: flex;
        justify-content: right;
    }
</style>